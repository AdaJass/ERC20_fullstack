{
	"name": "global-trade-network",
	"description": "The network to be in if you want to stay in the global trade business",
	"version": "1.0",
	"client": {
		"organization": "MEDSOS",
		"credentialStore": {
			"path": "/tmp/hfc-kvs",
			"cryptoStore": {
				"path": "/tmp/hfc-cvs"
			},
			"wallet": "wallet-name"
		}
	},
	"channels": {
		"ptwist": {
			"orderers": [
				"orderer.example.com"
			],
			"peers": {
				"peer0.MEDSOS.example.com": {
					"endorsingPeer":true,
					"chaincodeQuery":true,
					"ledgerQuery":true,
					"eventSource":true
				}
			},
			"chaincodes": [
				"ERC20:v1"
			]
		}
	},
	"organizations": {
		"MEDSOS": {
			"mspid": "MEDSOSMSP",
			"peers": [
				"peer0.MEDSOS.example.com"
			],
			"certificateAuthorities": [
				"ca.example.com"
			],
			"adminPrivateKeyPEM": "-----BEGIN PRIVATE KEY----- <etc>",
			"signedCertPEM": "-----BEGIN CERTIFICATE----- <etc>"
		}
	},
	"orderers": {
		"orderer.example.com": {
			"url_tmp": "grpcs://localhost:7050",
			"url": "grpc://localhost:7050",
			"grpcOptions": {
				"ssl-target-name-override": "orderer.example.com"
			},
			"tlsCACerts": {
				"path": "../../network/crypto-config/ordererOrganizations/example.com/orderers/orderer.example.com/msp/tlscacerts/tlsca.example.com-cert.pem"
			}
		}
	},
	"peers": {
		"peer0.MEDSOS.example.com": {
			"url_tmp": "grpcs://localhost:7051",
			"url": "grpc://localhost:7051",
			"grpcOptions": {
				"ssl-target-name-override": "peer0.MEDSOS.example.com"
			},
			"tlsCACerts": {
				"path": "../../network/crypto-config/peerOrganizations/MEDSOS.example.com/peers/peer0.MEDSOS.example.com/msp/tlscacerts/tlsca.MEDSOS.example.com-cert.pem"
			}
		}
	},
	"certificateAuthorities": {
		"ca.example.com": {
			"url_tmp": "https://localhost:7054",
			"url": "http://localhost:7054",
			"httpOptions": {
				"verify": false
			},
			"tlsCACerts": {
				"path": "../../network/crypto-config/peerOrganizations/MEDSOS.example.com/ca/ca.MEDSOS.example.com-cert.pem"
			},
			"registrar": [
				{
					"enrollId": "admin",
					"enrollSecret": "adminpw"
				}
			]
		}
	}
}
